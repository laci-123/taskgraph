{
    "compilerOptions":
    {
        "outDir": "./dist/",
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "esModuleInterop": true
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ]
}
